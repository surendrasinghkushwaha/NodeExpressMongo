<!DOCTYPE html>
<html>
  <head>
    <title>User List</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>User List</h1><a href="/newuser">Add New User</a>&nbsp; <a href="/">Back</a>
    <ul>
      <%
        var list = '';
        for (i = 0; i < userlist.length; i++) {
          list += '<li><a href="mailto:' + userlist[i].email + '">' + userlist[i].username + ' ['+ userlist[i].email + '] </a></li>';
        }
      %>
      <%- list %>
    </ul>
  </body>
</html>